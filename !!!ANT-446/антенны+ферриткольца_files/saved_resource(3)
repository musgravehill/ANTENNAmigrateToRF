window.uad_user_geo = {
	city: "Yaroslavl'",
	region: "Yaroslavskaya oblast'"
} ;

function uWrapperScriptLoaded(){
	var criteoLaunch = function(){
		var adUnits = {
			"placements": [{
				"slotid": "ad-unit-0",
				"zoneid": 1091587
			},{
				"slotid": "ad-unit-1",
				"zoneid": 1091588
			}]
		};
		Criteo.events.push(function() {
			Criteo.SetLineItemRanges("0..5000:1.00");
			Criteo.RequestBids(adUnits, function(response){
				window.criteo_response = response ;
				if(window.criteoLaunchDone) return ;
				window.criteoLaunchDone = 1 ;
				getNextScript() ;
			}, 1500);
		});
	};
	var getNextScript = function(){
		if(window.getNextScriptDone) return ;
		window.getNextScriptDone = 1 ;
		var _s = document.createElement('script');
		_s.src = "http://s203.ucoz.net/bnr/blocks/criteo_ucoz_rta_240x400.js?"+Math.random() ;
		document.getElementsByTagName('head')[0].appendChild(_s);
	} ;
	if(u_data[10]!='1') {
		var _s = document.createElement('script');
		_s.src = 'https://static.criteo.net/js/ld/publishertag.js' ;
		_s.onload = criteoLaunch ;
		_s.onerror = getNextScript ;
		document.getElementsByTagName('head')[0].appendChild(_s);
	} else {
		getNextScript() ;
	}
};

document.write('<scr'+'ipt type="text/javascript" src="http://s203.ucoz.net/bnr/wrappers/w1f.js" onload="uWrapperScriptLoaded();"></scr'+'ipt>');
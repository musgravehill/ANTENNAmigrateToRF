	function uPreroll_setcookie(){
		var s = document.createElement('script');
		s.src="//s203.ucoz.net/cgi/uutils.fcg?a=get_preroll_cookie&aa=1&r=0.717904216161067" ;
		document.head.appendChild(s);
	}

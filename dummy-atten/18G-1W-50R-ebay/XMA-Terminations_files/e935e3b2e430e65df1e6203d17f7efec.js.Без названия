{$.ajax({type:'post',url:url,beforeSend:function(){toggle.append(spinner);},success:function(data){toggle.find('.a-spinner').remove();personInfo.hide();bodyExpanded.html(data);bodyExpanded.addClass('expanded');bodyExpanded.show();},dataType:'html'});}});};}
